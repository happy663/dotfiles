{
  "global": {
    "ask_for_confirmation_before_quitting": true,
    "check_for_updates_on_startup": true,
    "show_in_menu_bar": true,
    "show_profile_name_in_menu_bar": false,
    "unsafe_ui": false
  },
  "profiles": [
    {
      "complex_modifications": {
        "parameters": {
          "basic.simultaneous_threshold_milliseconds": 50,
          "basic.to_delayed_action_delay_milliseconds": 500,
          "basic.to_if_alone_timeout_milliseconds": 1000,
          "basic.to_if_held_down_threshold_milliseconds": 500,
          "mouse_motion_to_scroll.speed": 100
        },
        "rules": [
          {
            "description": "Ctrl-j to Kana on PowerPoint",
            "manipulators": [
              {
                "conditions": [
                  {
                    "bundle_identifiers": [
                      "^com\\.microsoft\\.Powerpoint"
                    ],
                    "type": "frontmost_application_if"
                  }
                ],
                "from": {
                  "key_code": "j",
                  "modifiers": {
                    "mandatory": [
                      "control"
                    ]
                  }
                },
                "to": [
                  {
                    "key_code": "0",
                    "modifiers": [
                      "control",
                      "option"
                    ]
                  }
                ],
                "type": "basic"
              }
            ]
          },
          {
            "description": "Ctrl-h to delete on PowerPoint",
            "manipulators": [
              {
                "conditions": [
                  {
                    "bundle_identifiers": [
                      "^com\\.microsoft\\.Powerpoint"
                    ],
                    "type": "frontmost_application_if"
                  }
                ],
                "from": {
                  "key_code": "h",
                  "modifiers": {
                    "mandatory": [
                      "control"
                    ]
                  }
                },
                "to": [
                  {
                    "key_code": "delete_or_backspace"
                  }
                ],
                "type": "basic"
              }
            ]
          },
          {
            "description": "Ctrl+C and Ctrl+V to Cmd+C and Cmd+V and Cmd+W and Cmd+Z on PowerPoint & Chrome",
            "manipulators": [
              {
                "conditions": [
                  {
                    "bundle_identifiers": [
                      "^com\\.google\\.Chrome",
                      "^com\\.microsoft\\.Powerpoint",
                      "^com\\.tinyspeck\\.slackmacgap"
                    ],
                    "type": "frontmost_application_if"
                  }
                ],
                "from": {
                  "key_code": "c",
                  "modifiers": {
                    "mandatory": [
                      "control"
                    ],
                    "optional": [
                      "any"
                    ]
                  }
                },
                "to": [
                  {
                    "key_code": "c",
                    "modifiers": [
                      "command"
                    ]
                  }
                ],
                "type": "basic"
              },
              {
                "conditions": [
                  {
                    "bundle_identifiers": [
                      "^com\\.google\\.Chrome",
                      "^com\\.microsoft\\.Powerpoint",
                      "^com\\.tinyspeck\\.slackmacgap"
                    ],
                    "type": "frontmost_application_if"
                  }
                ],
                "from": {
                  "key_code": "v",
                  "modifiers": {
                    "mandatory": [
                      "control"
                    ],
                    "optional": [
                      "any"
                    ]
                  }
                },
                "to": [
                  {
                    "key_code": "v",
                    "modifiers": [
                      "command"
                    ]
                  }
                ],
                "type": "basic"
              },
              {
                "conditions": [
                  {
                    "bundle_identifiers": [
                      "^com\\.microsoft\\.Powerpoint",
                      "^com\\.google\\.Chrome",
                      "^com\\.tinyspeck\\.slackmacgap"
                    ],
                    "type": "frontmost_application_if"
                  }
                ],
                "from": {
                  "key_code": "w",
                  "modifiers": {
                    "mandatory": [
                      "control"
                    ],
                    "optional": [
                      "any"
                    ]
                  }
                },
                "to": [
                  {
                    "key_code": "w",
                    "modifiers": [
                      "command"
                    ]
                  }
                ],
                "type": "basic"
              },
              {
                "conditions": [
                  {
                    "bundle_identifiers": [
                      "^com\\.microsoft\\.Powerpoint",
                      "^com\\.google\\.Chrome",
                      "^com\\.tinyspeck\\.slackmacgap"
                    ],
                    "type": "frontmost_application_if"
                  }
                ],
                "from": {
                  "key_code": "z",
                  "modifiers": {
                    "mandatory": [
                      "control"
                    ],
                    "optional": [
                      "any"
                    ]
                  }
                },
                "to": [
                  {
                    "key_code": "z",
                    "modifiers": [
                      "command"
                    ]
                  }
                ],
                "type": "basic"
              }
            ]
          },
          {
            "description": "Remap Enter to Ctrl+Enter in ChatGPT",
            "manipulators": [
              {
                "conditions": [
                  {
                    "bundle_identifiers": [
                      "^com\\.openai\\.chat$"
                    ],
                    "type": "frontmost_application_if"
                  }
                ],
                "from": {
                  "key_code": "return_or_enter"
                },
                "to": [
                  {
                    "key_code": "return_or_enter",
                    "modifiers": [
                      "control"
                    ]
                  }
                ],
                "type": "basic"
              }
            ]
          },
          {
            "description": "Remap Ctrl+Enter to Enter in ChatGPT",
            "manipulators": [
              {
                "conditions": [
                  {
                    "bundle_identifiers": [
                      "^com\\.openai\\.chat$"
                    ],
                    "type": "frontmost_application_if"
                  }
                ],
                "from": {
                  "key_code": "return_or_enter",
                  "modifiers": {
                    "mandatory": [
                      "control"
                    ]
                  }
                },
                "to": [
                  {
                    "key_code": "return_or_enter"
                  }
                ],
                "type": "basic"
              }
            ]
          },
          {
            "description": "Alt+W to launch wezterm, disabled when wezterm is active",
            "manipulators": [
              {
                "type": "basic",
                "from": {
                  "key_code": "w",
                  "modifiers": {
                    "mandatory": [
                      "option"
                    ],
                    "optional": [
                      "any"
                    ]
                  }
                },
                "to": [
                  {
                    "shell_command": "open -a wezterm"
                  }
                ],
                "conditions": [
                  {
                    "type": "frontmost_application_unless",
                    "bundle_identifiers": [
                      "^com\\.github\\.wez\\.wezterm$"
                    ]
                  }
                ]
              }
            ]
          },
          {
            "description": "Ctrl+N to Down Arrow when pressed multiple times while holding Ctrl",
            "manipulators": [
              {
                "type": "basic",
                "from": {
                  "key_code": "n",
                  "modifiers": {
                    "mandatory": [
                      "control"
                    ],
                    "optional": [
                      "any"
                    ]
                  }
                },
                "to": [
                  {
                    "key_code": "down_arrow"
                  }
                ],
                "conditions": [
                  {
                    "type": "variable_if",
                    "name": "ctrl_pressed",
                    "value": 1
                  },
                  {
                    "type": "frontend_not",
                    "bundle_identifiers": [
                      "^com\\.github\\.wez\\.wezterm$"
                    ]
                  }
                ]
              },
              {
                "type": "basic",
                "from": {
                  "key_code": "n",
                  "modifiers": {
                    "mandatory": [
                      "control"
                    ],
                    "optional": [
                      "any"
                    ]
                  }
                },
                "to": [
                  {
                    "set_variable": {
                      "name": "ctrl_pressed",
                      "value": 1
                    }
                  },
                  {
                    "key_code": "n",
                    "modifiers": [
                      "control"
                    ]
                  }
                ],
                "to_after_key_up": [
                  {
                    "set_variable": {
                      "name": "ctrl_pressed",
                      "value": 0
                    }
                  }
                ],
                "conditions": [
                  {
                    "type": "frontend_not",
                    "bundle_identifiers": [
                      "^com\\.github\\.wez\\.wezterm$"
                    ]
                  }
                ]
              }
            ]
          }
        ]
      },
